{
  "expo": {
    "name": "Nutrential",
    "slug": "nutrient-app",
    "icon": "./assets/icon.jpeg",
    "version": "1.0.0",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "myapp",
    "android": {
      "package": "com.partiksingh.expostarterauthnativewind"
    }
  }
}
